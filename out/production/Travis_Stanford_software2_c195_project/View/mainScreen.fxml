<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainScreenController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="606.0" prefWidth="922.0">
         <children>
            <Label fx:id="welcomeLabel" alignment="CENTER" layoutX="246.0" layoutY="56.0" prefHeight="53.0" prefWidth="431.0" text="Welcome">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Button fx:id="appointmentsButton" layoutX="344.0" layoutY="184.0" mnemonicParsing="false" onAction="#appointmentsButtonHandler" prefHeight="72.0" prefWidth="235.0" text="Appointments">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="customersButton" layoutX="344.0" layoutY="293.0" mnemonicParsing="false" onAction="#customersButtonHandler" prefHeight="72.0" prefWidth="235.0" text="Customers">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="exitButton" layoutX="54.0" layoutY="522.0" mnemonicParsing="false" onAction="#exitButtonHandler" prefHeight="45.0" prefWidth="118.0" text="Exit">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="reportsButton" layoutX="344.0" layoutY="403.0" mnemonicParsing="false" onAction="#reportsButtonHandler" prefHeight="72.0" prefWidth="235.0" text="Reports">
