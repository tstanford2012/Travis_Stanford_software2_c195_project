<AnchorPane prefHeight="345.0" prefWidth="638.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TotalAppointmentsController">
   <children>
      <Button fx:id="backButton" layoutX="23.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonHandler" prefHeight="35.0" prefWidth="90.0" text="Back" />
      <Label layoutX="178.0" layoutY="82.0" prefHeight="42.0" prefWidth="361.0" text="View total customer appointments by:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="appointmentTypeRadBtn" layoutX="178.0" layoutY="161.0" mnemonicParsing="false" onMouseClicked="#appointmentTypeRadBtnHandler" text="Appointment Type" />
      <RadioButton fx:id="monthRadBtn" layoutX="391.0" layoutY="161.0" mnemonicParsing="false" onMouseClicked="#monthRadBtnHandler" text="Month" />
      <Button fx:id="calculateBtn" layoutX="298.0" layoutY="277.0" mnemonicParsing="false" onAction="#calculateBtnHandler" text="Calculate" />
      <ComboBox fx:id="customerComboBox" layoutX="256.0" layoutY="208.0" prefHeight="25.0" prefWidth="156.0" />
